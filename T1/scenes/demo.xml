<lxs>
    <scene root="ROOT NODE" axis_length="10"/>
    <views default="CAM1">
        <perspective id="CAM1" near="0.1" far="500" angle="20">
            <from x="20" y="30" z="20" />
            <to x="0" y="0" z="0" />
        </perspective>
        <perspective id="CAM2" near="0.1" far="10" angle="0">
            <from x="2" y="-20" z="1" />
            <to x="90" y="3" z="0" />
        </perspective>
    </views>




    <globals>
        <ambient    r="0.1" g="0.1"  b="0.1"  a="1.0" />
        <background r="0.8" g="1.0"  b="1.0"  a="1.0" />
    </globals>



    <lights>
        <omni id="L1" enabled="1">
            <location x="5.0" y="5.0" z="5.0" w="1.0" />
            <ambient  r="0.0" g="0.0" b="0.0" a="1.0" />
            <diffuse  r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </omni>
        <omni id="L2" enabled="1">
            <location x="20.0" y="40.0" z="20.0" w="1.0" />
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="0.0" g="0.0" b="1.0" a="1.0" />
        </omni>
    </lights>




    <textures>
        <texture id="default"       file="images/default.jpg" />
        <texture id="rickm1"        file="images/rm1.jpg" />
        <texture id="hex"           file="images/cyltex.jpg" />
        <texture id="legion"        file="images/legion.png" />
        <texture id="vidral"        file="images/vidral.jpg" />
        <texture id="trunk"         file="images/trunk-image.jpg" />
        <texture id="rocks"         file="images/rocks.jpg" />
        <texture id="leaves"        file="images/leaves.jpg" />
        <texture id="lightning"     file="images/lightning.png" />
        <texture id="space"         file="images/space.jpg" />
        <texture id="desk"          file="images/desk.jpg" />
        <texture id="floor"         file="images/floor.jpg" />
        <texture id="digital"       file="images/digital.png" />
    </textures>




    <materials>
        <material id="defaultMAT" shininess="10" >
            <emission r="0.0" g="0.0" b="0.0" a="1" />
            <ambient  r="1.0" g="1.0" b="1.0" a="1" />
            <diffuse  r="1.0" g="1.0" b="1.0" a="1" />
            <specular r="1.0" g="1.0" b="1.0" a="1" />
        </material>
        <material id="cyangloss" shininess="10">
            <emission r="0.0" g="0.0" b="0.0" a="1" />
            <ambient  r="0.1" g="1" b="1" a="1.0" />
            <diffuse  r="0.1" g="1" b="1" a="1.0" />
            <specular r="0.1" g="1" b="1" a="1.0" />
        </material>
        <material id="strongred" shininess="10" >
            <emission r="0.0" g="0.0" b="0.0" a="1" />
            <ambient  r="1.0" g="1.0" b="1.0" a="1" />
            <diffuse  r="0.0" g="0.0" b="0.0" a="0" />
            <specular r="0.0" g="0.0" b="0.0" a="1" />
        </material>
        <material id="metalMAT" shininess="10">
            <emission r="0.0" g="0.0" b="0.0" a="1" />
            <ambient  r="0.2" g="0.2" b="0.2" a="1" />
            <diffuse  r="0.51" g="0.51" b="0.51" a="1" />
            <specular r="0.51" g="0.51" b="0.51" a="0.51" />
        </material>
    </materials>





    <transformations>
        <!-- SQUEEZE TRANSFS -->
        <transformation id="squeeze_x">
            <scale x="0.1" y="1" z="1" />
        </transformation>
        <transformation id="squeeze_y">
            <scale x="1" y="0.1" z="1" />
        </transformation>
        <transformation id="squeeze_z">
            <scale x="1" y="1" z="0.1" />
        </transformation>
        <transformation id="squeeze_all">
            <scale x="0.1" y="0.1" z="0.1" />
        </transformation>


        <transformation id="torus1_setup">
            <scale x="1" y="1" z="1" />
            <rotate axis="x" angle="-90"/>
        </transformation>
        <transformation id="torus2_setup">
            <scale x="1" y="1" z="0.9" />
            <rotate axis="y" angle="90"/>
            <rotate axis="x" angle="-30"/>
        </transformation>
        <transformation id="torus3_setup">
            <scale x="1" y="1" z="0.9" />
            <rotate axis="y" angle="90"/>
            <rotate axis="x" angle="30"/>
        </transformation>
    </transformations>




    <primitives>
        <primitive id="rectangle">
            <rectangle x1="0" y1="0" x2="4" y2="4" />
        </primitive>
        <primitive id="sphere">
            <sphere radius="1" slices="30" stacks="30"/>
        </primitive>
        <primitive id="torus">
            <torus loops="50" slices="50" inner="0.2" outer="3.0"/>
        </primitive>
        <primitive id="cylinder">
            <cylinder slices="30" stacks="20" base="1" top="1" height="4" />
        </primitive>
    </primitives>




    <components>
        <component id="center-sph">
            <transformation>
            </transformation>
            <materials>
                <material id="cyangloss" />
            </materials>
            <texture id="space" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="sphere" />
            </children>
        </component>
        <component id="torus1">
            <transformation>
                <transformationref id="torus1_setup" />
            </transformation>
            <materials>
                <material id="cyangloss" />
            </materials>
            <texture id="legion" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="torus" />
            </children>
        </component>
        <component id="torus2">
            <transformation>
                <transformationref id="torus2_setup" />
            </transformation>
            <materials>
                <material id="cyangloss" />
            </materials>
            <texture id="legion" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="torus" />
            </children>
        </component>
        <component id="torus3">
            <transformation>
                <transformationref id="torus3_setup" />
            </transformation>
            <materials>
                <material id="cyangloss" />
            </materials>
            <texture id="legion" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="torus" />
            </children>
        </component>


        <component id="holder">
            <transformation>
                <rotate axis="x" angle="90"/>
                <scale x="0.2" y="0.2" z="2" />
            </transformation>
            <materials>
                <material id="cyangloss" />
            </materials>
            <texture id="none" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="cylinder" />
            </children>
        </component>


        <component id="RN-LOGO">
            <transformation>
                <translate x="0" y="5.24" z="0" />
                <scale x="1.2" y="1.2" z="1.2" />
            </transformation>
            <materials>
                <material id="metalMAT" />
            </materials>
            <texture id="default" length_s="1" length_t="1" />
            <children>
                <componentref id="center-sph" />
                <componentref id="torus1" />
                <componentref id="torus2" />
                <componentref id="torus3" />
                <!-- <componentref id="holder" /> -->
            </children>
        </component>

        <component id="floor">
            <transformation>
                <translate x="-20" y="0" z="20" />
                <scale x="10" y="10" z="10" />
                <rotate axis="x" angle="-90"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit" length_s="4" length_t="4" />
            <children>
                <primitiveref id="rectangle" />
            </children>
        </component>
    

        <component id="ROOT NODE">
            <transformation>
            </transformation>
            <materials>
                <material id="defaultMAT" />
            </materials>
            <texture id="digital" length_s="1" length_t="1" />
            <children>
                <!-- <componentref id="RN-LOGO" /> -->
                <componentref id="floor" />
            </children>
        </component>
    </components>
</lxs>